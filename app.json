{
  "expo": {
    "name": "Pizzaboi",
    "slug": "pizzaboi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-image-picker"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f65b1e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f65b1e"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.pizzaboi.pizzaboi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6cd984d0-98f2-4448-b10f-4b5ade954e1e"
      }
    }
  }
}
